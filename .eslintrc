{
  "parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
  "extends": "eslint:recommended",
	"globals": {
		"React": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"rules": {
		"semi": 2
	},
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  }
}
