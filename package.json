{
  "name": "spaced",
  "version": "0.0.0",
  "description": "spaced",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.babel.js -d --inline",
    "build": "rm -rf ./dist && webpack --config webpack.prod.config.babel.js && cp ./src/manifest.json ./dist && cp -f ./dist/index.html ./dist/200.html && git show >> ./dist/currentcommit.txt",
    "lint": "eslint .",
    "test": "npm run test.unit && npm run test.e2e.prod",
    "test.dev": "npm run test.unit && npm run test.e2e.dev",
    "test.unit": "karma start",
    "testd.unit": "karma start --auto-watch --no-single-run",
    "test.e2e.dev": "concurrently -k -s \"first\" \"npm start\" \"npm run protractor\"",
    "pretest.e2e.prod": "npm run build",
    "test.e2e.prod": "concurrently -k -s \"first\" \"npm run server.prod\" \"npm run protractor\"",
    "server.prod": "cd dist && angular-http-server || true"
  },
  "author": "Javier Pena <javier@javierdev.com>",
  "license": "MIT",
  "devDependencies": {
    "angular-http-server": "^1.0.6",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "sass-loader": "^4.0.2",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webdriver-manager": "^10.2.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "material-ui": "^0.16.2",
    "moment": "^2.16.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "uuid": "^3.0.0"
  }
}
